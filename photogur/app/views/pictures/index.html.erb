<p id="notice"><%= notice %></p>

<h1>Listing Pictures</h1>
              <div class="flip-3d"> 
<table id="cards">

  <thead>
    <tr>
      <th>Triva</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pictures.each do |picture| %>
      <tr>

 <td class="title"><%= picture.title %></td>
      <figure>  <td><img src="<%= picture.url %>"></td> </figure>
        <td><%= link_to 'Show', picture %></td>
        <td><%= link_to 'Edit', edit_picture_path(picture) %></td>
        <td><%= link_to 'Destroy', picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </div>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
